<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.du.hd_mybatis.StudentDao">
    <select id="Mybatis_SelectAllFun" resultType="com.du.hd_entity.Student">
        select * from student
    </select>

    <select id="Mybatis_SelectStudentByUsernameFun" resultType="com.du.hd_entity.Student">
        select * from student where name = #{username}
    </select>
</mapper>